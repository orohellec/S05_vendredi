<h1>Voic la liste des adhérents</h1>

<table class="table">
  <thead class="thead-dark">
   <tr>
       <th scope="col">#</th>
       <th id="prenom">Prénom</th>
       <th id="nom">Nom</th>
       <th id="email">email</th>
   </tr>
 </thead>
 <tbody>
<% i = 1 %>
<% @all_users.each do |user|%>
   <tr>
       <th scope="row"><%= i %></th>
       <td><%= user[:first_name] %></td>
       <td><%= user[:last_name] %></td>
       <td><%= user[:email] %></td>
   </tr>
   <% i += 1 %>
<% end %>
</tbody>
</table>
